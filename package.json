    "lodash": "^4.17.19",
    "mdi-material-ui": "^6.22.0",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "validator": "^13.5.2"
  },
  "description": "Design Language System (DLS) for ACT front-end projects.",
  "devDependencies": {
    "@babel/cli": "7.12.13",
    "@babel/core": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@react-theming/storybook-addon": "^1.0.3",
    "@storybook/addon-a11y": "6.1.18",
    "@storybook/addon-console": "1.2.3",
    "@storybook/addon-essentials": "6.1.18",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/addons": "6.1.18",
    "@storybook/react": "6.1.18",
    "@storybook/theming": "6.1.18",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/moment": "2.13.0",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.0",
    "@types/shelljs": "0.8.8",
    "@types/validator": "13.1.3",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-root-import": "6.6.0",
    "babel-plugin-transform-imports": "2.0.0",
    "eslint-config": "bitbucket:actet/eslint-config#v2.1.4",
    "eslint-import-resolver-babel-plugin-root-import": "1.1.1",
    "eslint-import-resolver-typescript": "2.3.0",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "prop-types": "^15.7.2",
    "react-docgen-typescript-loader": "3.7.2",
    "react-test-renderer": "17.0.1",
    "shelljs": "0.8.4",
    "ts-jest": "26.5.1",
    "ts-loader": "8.0.16",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "homepage": "https://bitbucket.org/actet/act-dls/#README",
  "keywords": [
    "design language system",
    "dls",
    "material-ui",
    "react",
    "storybook",
    "typescript"
  ],
  "lint-staged": {
    "*.{js}": "npm run test:lint:js",
    "*.{js,json,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": "npm run test:lint:ts",
    "*.md": "npm run test:lint:md"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "name": "@act/dls",
  "peerDependencies": {
    "@material-ui/core": "4.x",
    "@material-ui/data-grid": "4.x",
    "@material-ui/lab": "4.x",
    "react": "16.x || 17.x",
    "react-dom": "16.x || 17.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/actet/act-dls.git"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:clean": "rimraf dist",
    "build:components": "NODE_ENV=production babel src --extensions \".ts,.tsx\" --out-dir dist --config-file ./.babelrc.build.js",
    "build:icons": "bin/buildIcons",
    "build:types": "bin/buildTypes",
    "build:declarations": "tsc --project tsconfig.build.json",
    "build-storybook": "build-storybook",
    "build-storybook:dockerprep": "build-storybook -o ./storybook-static/www && cp Dockerfile ./storybook-static/Dockerfile",
    "build-storybook:docker": "cd storybook-static && docker build . -t dls-storybook",
    "start": "start-storybook -p 6006",
    "test": "npm-run-all test:**",
    "test:lint:js": "eslint . --ext .js --fix --quiet",
    "test:lint:md": "markdownlint README.md --config node_modules/eslint-config/markdownlint.config.json",
    "test:lint:ts": "eslint . --ext .ts,.tsx --fix --quiet",
    "test:types": "tsc --noEmit",
    "test:unit": "jest --maxWorkers=2 --silent"
  },
  "version": "4.0.0",
  "license": "MIT"
}
