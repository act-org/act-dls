  "dependencies": {
    "@material-ui/core": "4.10.2",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@material-ui/styles": "4.10.0",
    "lodash": "4.17.15",
    "mdi-material-ui": "6.16.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-jsx": "3.3.0"
  },
  "description": "Design Language System (DLS) for ACT front-end projects.",
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.10.1",
    "@babel/preset-typescript": "7.10.1",
    "@storybook/addon-info": "5.3.19",
    "@storybook/react": "5.3.19",
    "@testing-library/jest-dom": "5.10.0",
    "@testing-library/react": "10.2.1",
    "@types/jest": "26.0.0",
    "@types/lodash": "4.14.155",
    "@types/react": "16.9.36",
    "@types/react-dom": "16.9.8",
    "@types/styled-jsx": "2.2.8",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "babel-plugin-root-import": "6.5.0",
    "babel-plugin-transform-imports": "2.0.0",
    "eslint-config": "bitbucket:actet/eslint-config.git",
    "eslint-import-resolver-babel-plugin-root-import": "1.1.1",
    "eslint-import-resolver-typescript": "2.0.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.10",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "react-docgen-typescript-loader": "3.7.2",
    "react-test-renderer": "16.13.1",
    "ts-jest": "26.1.0",
    "ts-loader": "7.0.5",
    "typescript": "3.9.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "homepage": "https://bitbucket.org/actet/act-dls/#README",
  "keywords": [
    "design language system",
    "dls",
    "material-ui",
    "react",
    "storybook",
    "typescript"
  ],
  "lint-staged": {
    "*.{js}": "npm run test:lint:js",
    "*.{js,json,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": "npm run test:lint:ts",
    "*.md": "npm run test:lint:md"
  },
  "main": "lib/index.js",
  "name": "act-dls",
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/actet/act-dls.git"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:clean": "rimraf lib",
    "build:main": "NODE_ENV=production babel src --extensions \".ts,.tsx\" --ignore \"**/*.stories.tsx,**/*.test.tsx\" --out-dir lib",
    "build:types": "tsc --project tsconfig.build.json",
    "start": "start-storybook",
    "test": "npm-run-all test:**",
    "test:lint:js": "eslint . --ext .js --fix --quiet",
    "test:lint:md": "markdownlint README.md --config node_modules/eslint-config/markdownlint.config.json",
    "test:lint:ts": "eslint . --ext .ts,.tsx --fix --quiet",
    "test:types": "tsc --noEmit",
    "test:unit": "jest --maxWorkers=2 --silent"
  },
  "version": "0.0.4"
}
