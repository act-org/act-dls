  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "clsx": "^1.1.1",
    "lodash": "^4.17.19",
    "mdi-material-ui": "^6.20.0",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "validator": "^13.5.1"
  },
  "description": "Design Language System (DLS) for ACT front-end projects.",
  "devDependencies": {
    "@babel/cli": "7.12.8",
    "@babel/core": "7.12.9",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-react": "7.12.7",
    "@babel/preset-typescript": "7.12.7",
    "@storybook/addon-a11y": "5.3.19",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-backgrounds": "5.3.19",
    "@storybook/addon-console": "1.2.1",
    "@storybook/addon-info": "5.3.19",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/addon-viewport": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/react": "5.3.19",
    "@storybook/theming": "5.3.19",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@types/jest": "26.0.18",
    "@types/lodash": "4.14.165",
    "@types/moment": "2.13.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/shelljs": "0.8.8",
    "@types/validator": "13.1.1",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-root-import": "6.6.0",
    "babel-plugin-transform-imports": "2.0.0",
    "eslint-config": "bitbucket:actet/eslint-config#v2.1.3",
    "eslint-import-resolver-babel-plugin-root-import": "1.1.1",
    "eslint-import-resolver-typescript": "2.3.0",
    "husky": "4.3.5",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "react-docgen-typescript-loader": "3.7.2",
    "react-test-renderer": "17.0.1",
    "shelljs": "0.8.4",
    "storybook-addon-material-ui": "0.9.0-alpha.21",
    "storybook-host": "5.1.0",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.11",
    "ts-node": "9.1.1",
    "typescript": "4.1.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "homepage": "https://bitbucket.org/actet/act-dls/#README",
  "keywords": [
    "design language system",
    "dls",
    "material-ui",
    "react",
    "storybook",
    "typescript"
  ],
  "lint-staged": {
    "*.{js}": "npm run test:lint:js",
    "*.{js,json,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": "npm run test:lint:ts",
    "*.md": "npm run test:lint:md"
  },
  "main": "lib/index.js",
  "name": "act-dls",
  "peerDependencies": {
    "@material-ui/core": "4.x",
    "react": "16.x || 17.x",
    "react-dom": "16.x || 17.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/actet/act-dls.git"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:clean": "rimraf lib",
    "build:components": "NODE_ENV=production babel src/components --extensions \".ts,.tsx\" --out-dir lib/components",
    "build:constants": "NODE_ENV=production babel src/constants --extensions \".ts,.tsx\" --out-dir lib/constants",
    "build:helpers": "NODE_ENV=production babel src/helpers --extensions \".ts,.tsx\" --out-dir lib/helpers",
    "build:icons": "ts-node bin/buildIcons.mjs --experimental-modules",
    "build:styles": "NODE_ENV=production babel src/styles --extensions \".ts,.tsx\" --out-dir lib/styles",
    "build:types": "ts-node bin/buildTypes.mjs --experimental-modules",
    "build:declarations": "tsc --project tsconfig.build.json",
    "build-storybook": "build-storybook",
    "start": "start-storybook",
    "test": "npm-run-all test:**",
    "test:lint:js": "eslint . --ext .js --fix --quiet",
    "test:lint:md": "markdownlint README.md --config node_modules/eslint-config/markdownlint.config.json",
    "test:lint:ts": "eslint . --ext .ts,.tsx --fix --quiet",
    "test:types": "tsc --noEmit",
    "test:unit": "jest --maxWorkers=2 --silent"
  },
  "version": "2.1.5"
}
